				"typography": {
					"fontSize": "1.5rem",
					"textAlign": "center"
				},
				"spacing": {
					"margin": {
						"top": "16px"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"textAlign": "center"
				},
				"spacing": {
					"margin": {
						"top": "16px"
					}
				}
			}
		},
		"css": " & { overflow: hidden; }"
	}
