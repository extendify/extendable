{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#ffffff",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#2779a7",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#ac5039",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#faf8d4",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#fdfbf7",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"typography": {
				"font-size": {
					"headingOne": "clamp(3.5rem, 8vw, 5.4rem)",
					"headingTwo": "clamp(2.8rem, 6vw, 4.6rem)",
					"headingThree": "clamp(2.3rem, 4vw, 3.8rem)",
					"headingFour": "clamp(2.1rem, 3vw, 3rem)",
					"headingFive": "clamp(1.8rem, 3vw, 2.8rem)",
					"headingSix": "clamp(1.6rem, 3vw, 2.5rem)"
				},
				"font-weight": {
					"black": "900",
					"extraBold": "800",
					"bold": "700",
					"semiBold": "600",
					"medium": "490",
					"regular": "425",
					"light": "300",
					"extraLight": "210",
					"thin": "150"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.3,
					"medium": 1.5,
					"normal": 1.6
				},
				"rootSize": 16
			}
		},
		"layout": {
			"contentSize": "40em",
			"wideSize": "70em"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Gluten\", sans-serif",
					"name": "Gluten",
					"slug": "gluten",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Gluten",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": ["file:./assets/fonts/Gluten[ital,wght].woff2"]
						}
					]
				},
				{
					"fontFamily": "\"Radio Canada\", sans-serif",
					"name": "Radio Canada",
					"slug": "radio-canada",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Radio Canada",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/RadioCanada-VariableFont_wdth,wght.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.94rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.08rem, 2vw, 1.18rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.24rem, 2vw, 1.5rem)",
					"slug": "large"
				},
				{
					"name": "X-large",
					"size": "clamp(1.48rem, 2vw, 1.7rem)",
					"slug": "x-large"
				},
				{
					"name": "h1",
					"size": "var(--wp--custom--typography--font-size--heading-one)",
					"slug": "headingone"
				},
				{
					"name": "h2",
					"size": "var(--wp--custom--typography--font-size--heading-two)",
					"slug": "headingtwo"
				},
				{
					"name": "h3",
					"size": "var(--wp--custom--typography--font-size--heading-three)",
					"slug": "headingthree"
				},
				{
					"name": "h4",
					"size": "var(--wp--custom--typography--font-size--heading-four)",
					"slug": "headingfour"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": {
						"bottomLeft": "26px",
						"bottomRight": "7px",
						"topLeft": "12px",
						"topRight": "16px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "clamp(0.7rem, 1.5vw, 0.9rem)",
						"bottom": "clamp(0.7rem, 1.5vw, 0.8rem)",
						"right": "clamp(1.2rem, 2.5vw, 2rem)",
						"left": "clamp(1.2rem, 2.5vw, 2rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.04em"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/pullquote": {
				"border": {
					"radius": {
						"bottomLeft": "38px",
						"bottomRight": "153px",
						"topLeft": "68px",
						"topRight": "124px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--black)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"width": "15px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/social-links": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-one)",
					"fontWeight": "var(--wp--custom--typography--font-weight--thin)",
					"letterSpacing": "-0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"letterSpacing": "0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-four)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.04em",
					"textTransform": "normal"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gluten)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-six)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.1em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"blockGap": "min(30px, 5vw)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--radio-canada)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"title": "Brisbane",
	"version": 2
}
