{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#1f1f1f",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "var(--wp--preset--color--white, #fff)",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#0e4011",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#d6be04",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#edf5ee",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "var(--wp--style--block-gap, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.6rem, 5vw, 3.6rem)",
					"gigantic": "clamp(3rem, 6vw, 4.8rem)",
					"colossal": "clamp(3.6rem, 8vw, 5.6rem)"
				},
				"font-weight": {
					"black": "900",
					"extraBold": "800",
					"bold": "700",
					"semiBold": "600",
					"medium": "500",
					"regular": "400",
					"light": "300",
					"extraLight": "200",
					"thin": "100"
				},
				"line-height": {
					"tiny": 1.1,
					"small": 1.3,
					"medium": 1.5,
					"normal": 1.6
				}
			}
		},
		"layout": {
			"contentSize": "36em",
			"wideSize": "60em"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Roboto Flex\", sans-serif",
					"name": "Body (System Font)",
					"slug": "body-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Roboto Flex",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/RobotoFlex/RobotoFlex-VariableFont_GRAD,XTRA,YOPQ,YTAS,YTDE,YTFI,YTLC,YTUC,opsz,slnt,wdth,wght.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Roboto Flex\", serif",
					"name": "Headings (System Font)",
					"slug": "heading-font"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.94rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.08rem, 2vw, 1.15rem)",
					"slug": "medium"
				},
				{
					"name": "Large (H6)",
					"size": "clamp(1.8rem, 3vw, 2.3rem)",
					"slug": "large"
				},
				{
					"name": "X-large (H5)",
					"size": "clamp(2.18rem, 4vw, 2.85rem)",
					"slug": "x-large"
				},
				{
					"name": "2X Large (H4)",
					"size": "clamp(2.4rem, 4vw, 3.18rem)",
					"slug": "xx-large"
				},
				{
					"name": "Huge (H3)",
					"size": "var(--wp--custom--typography--font-size--huge, 4.5rem)",
					"slug": "huge"
				},
				{
					"name": "Gigantic (H2)",
					"size": "var(--wp--custom--typography--font-size--gigantic, 4.8rem)",
					"slug": "gigantic"
				},
				{
					"name": "Colossal (H1)",
					"size": "var(--wp--custom--typography--font-size--colossal, 5.4rem)",
					"slug": "colossal"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"radius": "0px",
					"width": "1px !important"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "clamp(0.5rem, 1.5vw, 0.7rem)",
						"bottom": "clamp(0.4rem, 1.5vw, 0.6rem)",
						"right": "clamp(1.25rem, 2.5vw, 1.5rem)",
						"left": "clamp(1.25rem, 2.5vw, 1.5rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-flex)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.03em",
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-flex)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/pullquote": {
				"color": {
					"background": "var(--wp--preset--color--tertiary)",
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"border": {
					"color": "var(--wp--preset--color--tertiary)",
					"width": "8px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-flex)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)",
					"fontWeight": "var(--wp--custom--typography--font-weight--thin)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"fontWeight": "var(--wp--custom--typography--font-weight--extra-light)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.01em",
					"textTransform": "normal"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.03em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "min(36px, 8vw)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--small)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"title": "Gdansk",
	"version": 2
}
