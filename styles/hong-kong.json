{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Hong Kong",
	"version": 2,
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": ["#c6120d", "#f1e3e1"],
					"slug": "foreground-and-background",
					"name": "Foreground and background"
				},
				{
					"colors": ["#f1e3e1", "#c6120d"],
					"slug": "background-and-foreground",
					"name": "Background and foreground"
				},
				{
					"colors": ["#dc1912", "#edc7c7"],
					"slug": "primary-and-secondary",
					"name": "Primary and secondary"
				}
			],
			"palette": [
				{
					"slug": "foreground",
					"color": "#c6120d",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#f1e3e1",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#dc1912",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#faf3f3",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#fbfbfb",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"elements": {
				"button": {
					"border": {
						"color": "var(--wp--preset--color--primary)"
					},
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--secondary)"
					},
					"spacing": {
						"padding": {
							"top": "1.4rem",
							"bottom": "1.4rem",
							"right": "2rem",
							"left": "2rem"
						}
					}
				}
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Space Mono\", Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace",
					"name": "Body (Space Mono)",
					"slug": "body",
					"fontFace": [
						{
							"fontFamily": "Space Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/SpaceMono/SpaceMono-Regular.woff2"]
						},
						{
							"fontFamily": "Space Mono",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/SpaceMono/SpaceMono-Bold.woff2"]
						}
					]
				},
				{
					"fontFamily": "\"Space Mono\", Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace",
					"name": "Headings (Space Mono)",
					"slug": "headings"
				}
			]
		},
		"layout": {
			"contentSize": "760px",
			"wideSize": "80rem"
		}
	},
	"styles": {
		"blocks": {
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "(--wp--custom--typography--font-weight--semi-bold)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)",
					"letterSpacing": "0"
				}
			},
			"h2": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"h4": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"h5": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
				}
			}
		}
	}
}
