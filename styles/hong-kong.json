{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#ffffff",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#212121",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#F2DF07",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#73726e",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#e6e6e6",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "var(--wp--style--block-gap, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"headingOne": "clamp(4.4rem, 8vw, 5.2rem)",
					"headingTwo": "clamp(3.8rem, 6vw, 4.8rem)",
					"headingThree": "clamp(2.8rem, 4vw, 3.9rem)",
					"headingFour": "clamp(2.3rem, 4vw, 3.2rem)",
					"headingFive": "clamp(2rem, 4vw, 2.7rem)",
					"headingSix": "clamp(1.8rem, 4vw, 2.2rem)"
				},
				"font-weight": {
					"black": "900",
					"extraBold": "800",
					"bold": "700",
					"semiBold": "600",
					"medium": "500",
					"regular": "400",
					"light": "300",
					"extraLight": "200",
					"thin": "100"
				},
				"line-height": {
					"tiny": 1.1,
					"small": 1.25,
					"medium": 1.4
				}
			}
		},
		"layout": {
			"contentSize": "40em",
			"wideSize": "68em"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Body (System Font)",
					"slug": "body-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": ["file:./assets/fonts/Inter/Inter-VariableFont_slnt,wght.woff2"]
						}
					]
				},
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Headings (System Font)",
					"slug": "heading-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": ["file:./assets/fonts/Montserrat-VariableFont_wght.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.95rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.07rem, 2vw, 1.18rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.18rem, 2vw, 1.4rem)",
					"slug": "large"
				},
				{
					"name": "X-large",
					"size": "clamp(1.8rem, 2vw, 3rem)",
					"slug": "x-large"
				},
				{
					"name": "h1",
					"size": "var(--wp--custom--typography--font-size--heading-one)",
					"slug": "headingone"
				},
				{
					"name": "h2",
					"size": "var(--wp--custom--typography--font-size--heading-two)",
					"slug": "headingtwo"
				},
				{
					"name": "h3",
					"size": "var(--wp--custom--typography--font-size--heading-three)",
					"slug": "headingthree"
				},
				{
					"name": "h4",
					"size": "var(--wp--custom--typography--font-size--heading-four)",
					"slug": "headingfour"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "999px",
					"width": "1px !important"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "clamp(0.9rem, 1.5vw, 1.1rem)",
						"bottom": "clamp(0.8rem, 1.5vw, 1rem)",
						"right": "clamp(1.6rem, 2.5vw, 2rem)",
						"left": "clamp(1.6rem, 2.5vw, 2rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.03em",
					"lineHeight": "1"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "0.01em"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--tertiary)"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"letterSpacing": "0.06em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "none"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"width": "7px"
				},
				"typography": {
					"fontSize": "var(--wp--custom--typography--font-size--large)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination-numbers": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"spacing": {
					"padding": {
						"bottom": "1rem",
						"top": "1rem",
						"left": "1rem",
						"right": "1rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"width": "5px"
				},
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--tertiary)"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.09em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-one)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.2em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.25em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-four)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.02em",
					"textTransform": "normal"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-six)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.18em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "min(40px, 9vw)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"title": "Hong Kong",
	"version": 2
}
