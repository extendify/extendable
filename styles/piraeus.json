{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "var(--wp--preset--color--white, #fff)",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#383838",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#f0ede6",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "var(--wp--preset--color--black, #000)",
					"name": "Secondary"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "var(--wp--style--block-gap, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"headingOne": "clamp(3.3rem, 8vw, 5rem)",
					"headingTwo": "clamp(3rem, 6vw, 4.5rem)",
					"headingThree": "clamp(2.6rem, 4vw, 3.8rem)",
					"headingFour": "clamp(2.4rem, 3vw, 3.18rem)",
					"headingFive": "clamp(2.18rem, 3vw, 2.9rem)",
					"headingSix": "clamp(1.8rem, 3vw, 2.5rem)"
				},
				"font-weight": {
					"black": "900",
					"extraBold": "800",
					"bold": "700",
					"semiBold": "600",
					"medium": "500",
					"regular": "400",
					"light": "300",
					"extraLight": "200",
					"thin": "100"
				},
				"line-height": {
					"tiny": 1.1,
					"small": 1.3,
					"medium": 1.5,
					"normal": 1.6
				}
			}
		},
		"layout": {
			"contentSize": "38em",
			"wideSize": "64em"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Roboto Flex\", sans-serif",
					"name": "Body (System Font)",
					"slug": "body-font",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Roboto Flex",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/RobotoFlex-VariableFont_GRAD,XTRA,YOPQ,YTAS,YTDE,YTFI,YTLC,YTUC,opsz,slnt,wdth,wght.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "\"Geom\", sans-serif",
					"name": "Headings (System Font)",
					"slug": "heading-font",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Geom",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 900",
							"src": ["file:./assets/fonts/Geom-Variable.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.94rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.08rem, 2vw, 1.15rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.24rem, 2vw, 1.5rem)",
					"slug": "large"
				},
				{
					"name": "X-large",
					"size": "clamp(1.48rem, 2vw, 1.7rem)",
					"slug": "x-large"
				},
				{
					"name": "h1",
					"size": "var(--wp--custom--typography--font-size--heading-one)",
					"slug": "headingone"
				},
				{
					"name": "h2",
					"size": "var(--wp--custom--typography--font-size--heading-two)",
					"slug": "headingtwo"
				},
				{
					"name": "h3",
					"size": "var(--wp--custom--typography--font-size--heading-three)",
					"slug": "headingthree"
				},
				{
					"name": "h4",
					"size": "var(--wp--custom--typography--font-size--heading-four)",
					"slug": "headingfour"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--foreground) !important",
					"radius": "0px",
					"width": "2px !important"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)"
				},
				"spacing": {
					"padding": {
						"top": "clamp(0.8rem, 1.5vw, 1rem)",
						"bottom": "clamp(0.6rem, 1.5vw, 0.8rem)",
						"right": "clamp(1.3rem, 2.5vw, 1.6rem)",
						"left": "clamp(1.3rem, 2.5vw, 1.6rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.02em",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--body-font)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"width": "6px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)"
				}
			},
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-one)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"letterSpacing": "-0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"letterSpacing": "0.04em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-four)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.01em",
					"textTransform": "normal"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-six)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.03em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "min(48px, 6vw)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"title": "Piraeus",
	"version": 2
}
