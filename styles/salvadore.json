{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "foreground",
					"color": "#ffffff",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#000000",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#dbbf97",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#9c7c4f",
					"name": "Secondary"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "var(--wp--style--block-gap, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"headingOne": "clamp(4.4rem, 8vw, 6.8rem)",
					"headingTwo": "clamp(3.8rem, 6vw, 5.6rem)",
					"headingThree": "clamp(2.6rem, 4vw, 3.8rem)",
					"headingFour": "clamp(2.3rem, 4vw, 3.2rem)",
					"headingFive": "clamp(2.18rem, 4vw, 3rem)",
					"headingSix": "clamp(2rem, 4vw, 2.5rem)"
				},
				"font-weight": {
					"black": "900",
					"extraBold": "800",
					"bold": "700",
					"semiBold": "600",
					"medium": "500",
					"regular": "400",
					"light": "300",
					"extraLight": "200",
					"thin": "100"
				},
				"line-height": {
					"tiny": 1.1,
					"small": 1.25,
					"medium": 1.4
				}
			}
		},
		"layout": {
			"contentSize": "36em",
			"wideSize": "60em"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Montserrat (System Font)",
					"slug": "body-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 700",
							"src": ["file:./assets/fonts/Montserrat-VariableFont_wght.woff2"]
						}
					]
				},
				{
					"fontFamily": "\"Josefin Sans\", sans-serif",
					"name": "Headings (System Font)",
					"slug": "heading-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Josefin Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": ["file:./assets/fonts/JosefinSans-VariableFont_wght.woff2"]
						}
					]
				},
				{
					"fontFamily": "\"Mrs Saint Delafield\", cursive",
					"name": "Handwriting (System Font)",
					"slug": "handwriting-font",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Mrs Saint Delafield",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/MrsSaintDelafield-Regular.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.94rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.1rem, 2vw, 1.25rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.24rem, 2vw, 1.5rem)",
					"slug": "large"
				},
				{
					"name": "X-large",
					"size": "clamp(2.5rem, 2vw, 4rem)",
					"slug": "x-large"
				},
				{
					"name": "h1",
					"size": "var(--wp--custom--typography--font-size--heading-one)",
					"slug": "headingone"
				},
				{
					"name": "h2",
					"size": "var(--wp--custom--typography--font-size--heading-two)",
					"slug": "headingtwo"
				},
				{
					"name": "h3",
					"size": "var(--wp--custom--typography--font-size--heading-three)",
					"slug": "headingthree"
				},
				{
					"name": "h4",
					"size": "var(--wp--custom--typography--font-size--heading-four)",
					"slug": "headingfour"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px",
					"width": "1px !important"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "clamp(0.7rem, 1.5vw, 0.9rem)",
						"bottom": "clamp(0.6rem, 1.5vw, 0.8rem)",
						"right": "clamp(1.4rem, 2.5vw, 1.8rem)",
						"left": "clamp(1.4rem, 2.5vw, 1.8rem)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.03em",
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "0.01em"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"style": "double none double none",
					"width": "9px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--large)"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination-numbers": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)"
				}
			},
			"core/query-pagination-previous": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"style": "none none none double",
					"width": "7px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--handwriting-font)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"letterSpacing": "0.02em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--handwriting-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-one)",
					"fontWeight": "var(--wp--custom--typography--font-weight--light)",
					"letterSpacing": "-0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--handwriting-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-two)",
					"fontWeight": "var(--wp--custom--typography--font-weight--regular)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-three)",
					"fontWeight": "var(--wp--custom--typography--font-weight--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-four)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.01em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-five)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)",
					"letterSpacing": "0.01em",
					"textTransform": "normal"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-six)",
					"fontWeight": "var(--wp--custom--typography--font-weight--semi-bold)",
					"letterSpacing": "0.03em",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "min(36px, 8vw)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--typography--font-weight--regular)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"title": "Salvador",
	"version": 2
}
