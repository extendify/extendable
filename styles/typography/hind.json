{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Hind",
	"version": 3,
	"slug": "hind",
	"styles": {
		"blocks": {
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
